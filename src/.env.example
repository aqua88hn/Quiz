# DATABASE - PostgreSQL Local
DATABASE_URL="postgresql://<connection>"
DB_HOST=localhost
DB_PORT=5432
DB_USER=<pg-db-user>
DB_PASSWORD=<password>
DB_NAME=<dbname>

# LOGGING
LOG_LEVEL=debug
LOG_REDACT_FIELDS=password,token,authorization,credit_card,email

# AUTHENTICATION
JWT_SECRET=<your-secret-key-min-32-chars-required>
ADMIN_PASSWORD=admin123

# APPLICATION
NODE_ENV=development
API_URL=http://localhost:3000
API_TIMEOUT=30000

# RATE LIMITING
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# APP INFO
NEXT_PUBLIC_APP_NAME=<NameApp>
NEXT_PUBLIC_APP_VERSION=1.0.0

# REDIS (Optional - for caching)
# REDIS_URL=redis://localhost:6379

# SENTRY (Optional - for error tracking in production)
# SENTRY_DSN=

# UPSTASH (Optional - if using Upstash Redis)
# UPSTASH_KV_REST_API_URL=
# UPSTASH_KV_REST_API_TOKEN=